Cell[BoxData[{
 RowBox[{"phi10", "=", 
  RowBox[{
   RowBox[{"1", "/", "2"}], " ", 
   RowBox[{"(", 
    RowBox[{"ArrayFlatten", "[", 
     RowBox[{"TensorProduct", "[", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"const1", "*", "up"}], "+", 
         RowBox[{"const2", "*", "down"}]}], ")"}], ",", 
       RowBox[{"(", 
        RowBox[{"up", "+", "down"}], ")"}]}], "]"}], "]"}], ")"}]}]}], "\n", 
 RowBox[{"phi10c", "=", 
  RowBox[{"Transpose", "[", "phi10", "]"}]}], "\n", 
 RowBox[{"outer10", "=", 
  RowBox[{"ArrayFlatten", "[", 
   RowBox[{"Outer", "[", 
    RowBox[{"Times", ",", "phi10", ",", "phi10c"}], "]"}], "]"}]}], "\n", 
 RowBox[{"Xb10", "=", 
  RowBox[{"Transpose", "[", 
   RowBox[{"Eigenvectors", "[", "outer10", "]"}], "]"}]}], "\n", 
 RowBox[{"Db10", "=", 
  RowBox[{"DiagonalMatrix", "[", 
   RowBox[{"Eigenvalues", "[", "outer10", "]"}], "]"}]}], "\n", 
 RowBox[{"check10", "=", 
  RowBox[{
   RowBox[{"Xb10", ".", "Db10", ".", 
    RowBox[{"Inverse", "[", "Xb10", "]"}]}], "//", "FullSimplify"}]}], "\n", 
 RowBox[{
  RowBox[{"stlist10", "=", 
   RowBox[{"Select", "[", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"outer10", "-", "check10"}], "//", "Flatten"}], "//", 
      "Union"}], ",", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"#", "=!=", "0"}], "&"}], ")"}]}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"stlist10", "//", "FullSimplify"}], "\n"}], "\n"}], "Input",
